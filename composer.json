{
    "name": "kaliel/cakephp-image",
    "description": "Image plugin for CakePHP",
    "type": "cakephp-plugin",
    "homepage": "https://github.com/kaliel86/cakephp-image",
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "cakephp/cakephp": "^3.7",
        "intervention/image": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Image\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Image\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
    "support": {
        "source": "https://github.com/kaliel86/cakephp-image"
    }
}
